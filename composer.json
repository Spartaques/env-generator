{
    "name": "spartaques/php-horizon-configs-loader",
    "license": "MIT",
    "description": "Configs loader",
    "type": "library",
    "authors": [
        {
            "name": "Andriy Bashuk",
            "email": "abashuk@newagesol.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-zookeeper": "*",
        "symfony/yaml": "~4.1.0",
        "kyoz/zookeeper-client": "~1.0",
        "larapack/dd": "~1.1",
        "illuminate/support": "~5.7"
    },
    "autoload": {
        "psr-4": {
